{
    "functions": {
        "api\/*.php": {
            "runtime": "vercel-php@0.7.4"
        }
    },
    "env": {
        "APP_ENV": "prod"
    },
    "routes": [
        {
            "src": "\/(assets)\/(.*)$",
            "dest": "\\\/api\\\/assets.php?path=\\\/$2"
        },
        {
            "src": "\/(.*)",
            "dest": "\/api\/index.php"
        }
    ]
}